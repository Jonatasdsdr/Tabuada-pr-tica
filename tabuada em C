#include <stdio.h>
#include <locale.h>

int main (){
	
	setlocale (LC_ALL, "Portuguese");
	printf ("Tabuada\n\n\n\n");
	
	int i, tabuada;
	
	printf ("Digite a tabuada que vocÃª deseja:\t");
	scanf ("%d", &tabuada);
	
	for (i=0; i<10; i++){
		printf ("%d x %d = %d\n", i, tabuada, i * tabuada);
	}
	
	system ("pause");
}
